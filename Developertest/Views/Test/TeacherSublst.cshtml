@model Developertest.Models.Test

@{
    ViewBag.Title = "TeacherSublst";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Teachers Subject List</h2>

<p>
    @Html.ActionLink("Create New", "TeacherSubName")
</p>
<table class="table">
    <tr>
      
        <th>
            @Html.DisplayNameFor(model => model.TeacherName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubjectName)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Class)
        </th>
        
    </tr>

@foreach (var item in Model.Studentlst) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.TeacherName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubjectName)
        </td>
    
        <td>
            @Html.DisplayFor(modelItem => item.Class)
        </td>
       
        
    </tr>
}

</table>
